#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      char a[100];
    int i,l=0;
    scanf("%[^\n]%*c",a);
    l=strlen(a);
    for(i=l-1;i>=0;i--){
        printf("%c",a[i]);
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
